<configuração>
    <system.webServer>
      <rewrite>
        <rules>
          <rule name = "Angular" stopProcessing = "true">
            <match url = ". *" />
            <condições logicalGrouping = "MatchAll">
              <adicionar input = "{REQUEST_FILENAME}" matchType = "IsFile" negate = "true" />
              <adicionar input = "{REQUEST_FILENAME}" matchType = "IsDirectory" negate = "true" />
            </conditions>
            <action type = "Rewrite" url = "/" />
          </rule>
        </rules>
      </rewrite>
    </system.webServer>
</configuration>